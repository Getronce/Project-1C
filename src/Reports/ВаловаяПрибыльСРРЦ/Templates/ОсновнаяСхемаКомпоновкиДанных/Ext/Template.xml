;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ЕСТЬNULL(ЗакупочныеЦены.Цена, 0) КАК ЗакупочнаяЦена,
	ЗакупочныеЦены.Номенклатура КАК Номенклатура
ПОМЕСТИТЬ ВТ_ЗакупочнаяЦена
ИЗ
	РегистрСведений.ЦеныНоменклатуры.СрезПоследних(&amp;КонецПериода, ВидЦены = &amp;ВидЦеныЗакупочная) КАК ЗакупочныеЦены
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ЕСТЬNULL(РозничныеЦены.Цена, 0) КАК РРЦ,
	РозничныеЦены.Номенклатура КАК Номенклатура
ПОМЕСТИТЬ ВТ_РозничнаяЦена
ИЗ
	РегистрСведений.ЦеныНоменклатуры.СрезПоследних(&amp;КонецПериода, ВидЦены = &amp;ВидЦеныРозничная) КАК РозничныеЦены
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТ_ДанныеПродаж.Количество * ЕСТЬNULL(ВТ_ЗакупочнаяЦена.ЗакупочнаяЦена, 0) КАК Себестоимость,
	ВТ_ДанныеПродаж.СуммаПродаж - ВТ_ДанныеПродаж.Количество * ЕСТЬNULL(ВТ_ЗакупочнаяЦена.ЗакупочнаяЦена, 0) КАК ВаловаяПрибыль,
	ВЫБОР
		КОГДА ВТ_ДанныеПродаж.Количество * ЕСТЬNULL(ВТ_ЗакупочнаяЦена.ЗакупочнаяЦена, 0) = 0
			ТОГДА 100
		ИНАЧЕ (ВТ_ДанныеПродаж.СуммаПродаж - ВТ_ДанныеПродаж.Количество * ЕСТЬNULL(ВТ_ЗакупочнаяЦена.ЗакупочнаяЦена, 0)) / (ВТ_ДанныеПродаж.Количество * ЕСТЬNULL(ВТ_ЗакупочнаяЦена.ЗакупочнаяЦена, 0)) * 100
	КОНЕЦ КАК РентабельностьПродаж,
	ВЫБОР
		КОГДА ВТ_ДанныеПродаж.СуммаПродаж = 0
			ТОГДА 100
		ИНАЧЕ (ВТ_ДанныеПродаж.СуммаПродаж - ВТ_ДанныеПродаж.Количество * ЕСТЬNULL(ВТ_ЗакупочнаяЦена.ЗакупочнаяЦена, 0)) / ВТ_ДанныеПродаж.СуммаПродаж * 100
	КОНЕЦ КАК ЭффективностьПродаж,
	ЕСТЬNULL(ВТ_РозничнаяЦена.РРЦ, 0) КАК РРЦ,
	ВТ_ДанныеПродаж.Количество КАК Количество,
	ВТ_ДанныеПродаж.СуммаПродаж КАК СуммаПродаж,
	ВТ_ДанныеПродаж.Менеджер КАК Менеджер,
	ВТ_ДанныеПродаж.Номенклатура КАК Номенклатура
ИЗ
	ВТ_ДанныеПродаж КАК ВТ_ДанныеПродаж
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_ЗакупочнаяЦена КАК ВТ_ЗакупочнаяЦена
		ПО ВТ_ДанныеПродаж.Номенклатура = ВТ_ЗакупочнаяЦена.Номенклатура
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_РозничнаяЦена КАК ВТ_РозничнаяЦена
		ПО ВТ_ДанныеПродаж.Номенклатура = ВТ_РозничнаяЦена.Номенклатура</query>
